}
@article{Bastida2009,
abstract = {Soil is a dynamic system in which microorganisms perform important tasks in organic matter transformations and nutrient cycles. Recently, some studies have started to focus on soil metaproteomics as a tool for understanding the function and the role of members of the microbial community. The aim of our work was to provide a review of soil proteomics by looking at the methodologies used in order to illustrate the challenges and gaps in this field, and to provide a broad perspective about the use and meaning of soil metaproteomics. The development of soil metaproteomics is influenced strongly by the extraction methods. Several methods are available but only a few provide an identification of soil proteins, while others extract proteins and are able to separate them by electrophoresis but do not provide an identification. The extraction of humic compounds together with proteins interferes with the latter's separation and identification, although some methods can avoid these chemical interferences. Nevertheless, the major problems regarding protein identification reside in the fact that soil is a poor source of proteins and that there is not enough sequence-database information for the identification of proteins by mass spectrometric analysis. Once these pitfalls have been solved, the identification of soil proteins may provide information about the biogeochemical potential of soils and pollutant degradation and act as an indicator of soil quality, identifying which proteins and microorganisms are affected by a degradation process. The development of soil metaproteomics opens the way to proteomic studies in other complex substrates, such as organic wastes. These studies can be a source of knowledge about the possibility of driven soil restoration in polluted and degraded areas with low organic matter content and even for the identification of enzymes and proteins with a potential biotechnological value.},
author = {Bastida, F. and Moreno, J. L. and Nicol{\'{a}}s, C. and Hern{\'{a}}ndez, T. and Garc{\'{i}}a, C.},
doi = {10.1111/j.1365-2389.2009.01184.x},
file = {:C$\backslash$:/Users/Jo{\~{a}}o Sequeira/Documents/Escola/Tese/articles/metaproteomics/3.2. Soil metaproteomics. a review of an emerging.pdf:pdf},
isbn = {1351-0754},
issn = {13510754},
journal = {European Journal of Soil Science},
number = {6},
pages = {845--859},
title = {{Soil metaproteomics: A review of an emerging environmental science. Significance, methodology and perspectives}},
volume = {60},
year = {2009}
}
@article{Benson2011,
abstract = {Fumaroles, commonly called steam vents, are ubiquitous features of geothermal habitats. Recent studies have discovered microorganisms in condensed fumarole steam, but fumarole deposits have proven refractory to DNA isolation. In this study, we report the development of novel DNA isolation approaches for fumarole deposit microbial community analysis. Deposit samples were collected from steam vents and caves in Hawaii Volcanoes National Park, Yellowstone National Park and Lassen Volcanic National Park. Samples were analyzed by X-ray microanalysis and classified as nonsulfur, sulfur or iron-dominated steam deposits. We experienced considerable difficulty in obtaining high-yield, high-quality DNA for cloning: only half of all the samples ultimately yielded sequences. Analysis of archaeal 16S rRNA gene sequences showed that sulfur steam deposits were dominated by Sulfolobus and Acidianus, while nonsulfur deposits contained mainly unknown Crenarchaeota. Several of these novel Crenarchaeota lineages were related to chemoautotrophic ammonia oxidizers, indicating that fumaroles represent a putative habitat for ammonia-oxidizing Archaea. We also generated archaeal and bacterial enrichment cultures from the majority of the deposits and isolated members of the Sulfolobales. Our results provide the first evidence of Archaea in geothermal steam deposits and show that fumaroles harbor diverse and novel microbial lineages.},
author = {Benson, Courtney A. and Bizzoco, Richard W. and Lipson, David A. and Kelley, Scott T.},
doi = {10.1111/j.1574-6941.2011.01047.x},
file = {:C$\backslash$:/Users/Jo{\~{a}}o Sequeira/Documents/Escola/Tese/articles/metagenomics/benson2011.pdf:pdf},
isbn = {1574-6941 (Electronic)$\backslash$r0168-6496 (Linking)},
issn = {01686496},
journal = {FEMS Microbiology Ecology},
keywords = {16S,Crenarchaeota,Microbial community,Phylogeny,Sulfolobus},
number = {1},
pages = {74--88},
pmid = {21223339},
title = {{Microbial diversity in nonsulfur, sulfur and iron geothermal steam vents}},
volume = {76},
year = {2011}
}
@article{Bikel2015a,
abstract = {The advances in experimental methods and the development of high performance bioinformatic tools have substantially improved our understanding of microbial communities associated with human niches. Many studies have documented that changes in microbial abundance and composition of the human microbiome is associated with human health and diseased state. The majority of research on human microbiome is typically focused in the analysis of one level of biological information, i.e., metagenomics or metatranscriptomics. In this review, we describe some of the different experimental and bioinformatic strategies applied to analyze the 16S rRNA gene profiling and shotgun sequencing data of the human microbiome. We also discuss how some of the recent insights in the combination of metagenomics, metatranscriptomics and viromics can provide more detailed description on the interactions between microorganisms and viruses in oral and gut microbiomes. Recent studies on viromics have begun to gain importance due to the potential involvement of viruses in microbial dysbiosis. In addition, metatranscriptomic combined with metagenomic analysis have shown that a substantial fraction of microbial transcripts can be differentially regulated relative to their microbial genomic abundances. Thus, understanding the molecular interactions in the microbiome using the combination of metagenomics, metatranscriptomics and viromics is one of the main challenges towards a system level understanding of human microbiome.},
author = {Bikel, Shirley and Valdez-Lara, Alejandra and Cornejo-Granados, Fernanda and Rico, Karina and Canizales-Quinteros, Samuel and Sober{\'{o}}n, Xavier and {Del Pozo-Yauner}, Luis and Ochoa-Leyva, Adri{\'{a}}n},
doi = {10.1016/j.csbj.2015.06.001},
file = {:C$\backslash$:/Users/Jo{\~{a}}o Sequeira/Documents/Escola/Tese/articles/metatranscriptomics/2.3. Combining metagenomics, metatranscriptomics and viromics to explore.pdf:pdf},
issn = {20010370},
journal = {Computational and Structural Biotechnology Journal},
keywords = {Bioinformatics,Human microbiome,Metagenomics,Metatranscriptomics,Systems-level,Viromics},
pages = {390--401},
pmid = {26137199},
publisher = {Elsevier B.V.},
title = {{Combining metagenomics, metatranscriptomics and viromics to explore novel microbial interactions: Towards a systems-level understanding of human microbiome}},
volume = {13},
year = {2015}
}
@article{Blackburn2016,
abstract = {Increasing evidence supports the notion that the human microbiome is intimately linked to human health as well as to disease, a corollary being that manipulation of the microbiome might provide a means to improve future health and treatment outcomes. The ability to quantify dynamic changes in the concentration of potential microbial effectors - proteins, lipids and small molecule metabolites - produced by the microbiome should complement genomic approaches to the documentation of microbial composition and abundance, but is limited by current data analysis approaches. Elucidating the physiological role of such microbial factors may yield unexpected new drug and vaccine targets in a range of non-communicable diseases, as well as opening up the possibility of new chemical supplementation-based treatment regimes.},
author = {Blackburn, Jonathan M. and Martens, Lennart},
doi = {10.1586/14789450.2016.1135058},
file = {:C$\backslash$:/Users/Jo{\~{a}}o Sequeira/Documents/Escola/Tese/articles/metaproteomics/3.1. The challenge of metaproteomic analysis inhuman samples.pdf:pdf},
isbn = {1478-9450},
