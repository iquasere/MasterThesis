keywords = {Archaea,Archaea: classification,Archaea: genetics,Archaea: isolation {\&} purification,Bacteria,Bacteria: classification,Bacteria: genetics,Bacteria: isolation {\&} purification,Gene Expression Profiling,Metagenome,Soil,Soil Microbiology,Soil: chemistry},
