doi = {10.3402/jom.v6.25443},
file = {:C$\backslash$:/Users/Asus/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sim{\'{o}}n-Soro, Guillen-Navarro, Mira - 2014 - Metatranscriptomics reveals overall active bacterial composition in caries lesions.pdf:pdf},
isbn = {2000-2297},
