journal = {Handbook of Molecular Microbial Ecology II: Metagenomics in Different Habitats},
keywords = {Amplicon libraries sequenced, to depths,Deep sea microbial diversity, "Rare Biosphere",Rare biosphere, feedback mechanisms},
pages = {243--252},
