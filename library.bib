file = {:C$\backslash$:/Users/Asus/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fosso et al. - 2015 - BioMaS a modular pipeline for Bioinformatic analysis of Metagenomic AmpliconS.pdf:pdf},
