isbn = {1872-7565 (Electronic)$\backslash$r0169-2607 (Linking)},
issn = {01692607},
journal = {Computer Methods and Programs in Biomedicine},
keywords = {Application framework,Biomedical informatics,Open software,Reusable component model,Scientific software development},
number = {2},
pages = {191--203},
pmid = {20047774},
title = {{AIBench: A rapid application development framework for translational research in biomedicine}},
